  "scripts": {
    "compose:up": "docker compose up -d",
    "compose:down": "docker compose down",
    "develepment-launcher": "npm run scss:watch & npm run start-launcher:debug",
    "develepment-server": "npm run scss:watch & npm run start-server:debug",
    "production": "npm run start-server:production",
    "build": "npm run scss:build",
    "start-server:debug": "nodemon -r dotenv/config --inspect=0.0.0.0 src/server.js",
    "start-launcher:debug": "nodemon -r dotenv/config --inspect=0.0.0.0 src/launcher.js",
    "start-launcher:production": "node -r dotenv/config src/launcher.js",
    "scss:watch": "sass --watch src/scss:src/public/css",
    "scss:build": "sass src/scss:src/public/css"
  },
