<div class="alert-info error">
    <img src="/icons/red.info-circle.svg" alt="<%= __('alert.error_icon_alt') %>">
    <% if (moreInfoLink) { %>
        <p><%= info %> <a href="<%= moreInfoLink %> "><%= __("alert.learn_more") %></a></p>
    <% } else { %>
        <p><%= info %></p>
    <% } %>
</div>
