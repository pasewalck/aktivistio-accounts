<div class="input-checkbox<% if (errors[checkbox.name]) { %> errored<% } %>">
    <input <%= checkbox.required ? "required" : "" %> type="checkbox" name="<%= checkbox.name %>"
        <% if (!checkbox.blockValue && (formData[checkbox.name] || errors[checkbox.name]?.value)) { %>checked<% } %>
    >
    <label><%= checkbox.msg %></label> 
</div>
<% if (errors[checkbox.name]) { %>
    <% errors[checkbox.name].rendered = true %> 
    <p class="input-checkbox-error"><%= errors[checkbox.name].msg %></p>
<% } %>
